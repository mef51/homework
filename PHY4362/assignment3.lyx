\begin_layout Problem
Prove that the electric quadrupole moment of a uniformly charged ellipsoid
 of semi-major axis b and semi-minor axis a is given by 
\begin_inset Formula $eQ=\frac{2}{5}Ze(b^{2}-a^{2})$
\end_inset


\end_layout

\begin_layout Standard
First, let's map the x, y, z coordinates of the ellipse to a unit sphere:
 
\begin_inset Formula $(x,y,z)=(au,av,bw)$
\end_inset

 so that
\begin_inset Formula 
\[
\begin{aligned}u=x/a\\
v=y/a\\
w=z/b
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
The Jacobian of this transformation is 
\begin_inset Formula $a\cdot a\cdot b=a^{2}b$
\end_inset

.
\end_layout

\begin_layout Standard
Now
\begin_inset Formula 
\[
\begin{aligned}eQ & = & e\int\psi^{*}(3z^{2}-r^{2})\psi dV\\
 & = & e\int\psi^{*}(3z^{2}-x^{2}-y^{2}-z^{2})\psi dV\\
 & = & e\int\psi^{*}(2z^{2}-x^{2}-y^{2})\psi dV\\
 & = & ea^{2}b\int\psi^{*}(2b^{2}w^{2}-a^{2}u^{2}-a^{2}v^{2})\psi dV &  &  &  & (change\, coordinates)\\
 & = & ea^{2}b(2b^{2}\int\psi^{*}w^{2}\psi dV-a^{2}\int\psi^{*}u^{2}\psi dV-a^{2}\int\psi^{*}v^{2}\psi dV)\\
 & = & ea^{2}b(2b^{2}<w^{2}>-a^{2}<u^{2}>-a^{2}<v^{2}>) &  &  &  & (expectation\, values)
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
Since 
\begin_inset Formula $u$
\end_inset

, 
\begin_inset Formula $v$
\end_inset

, and 
\begin_inset Formula $w$
\end_inset

 are in a sphere, 
\begin_inset Formula $<u^{2}>=<v^{2}>=<w^{2}>$
\end_inset

.
 So,
\begin_inset Formula 
\[
\begin{aligned}eQ & = & ea^{2}b(2b^{2}<w^{2}>-a^{2}<u^{2}>-a^{2}<v^{2}>)\\
 & = & ea^{2}b(2b^{2}-2a^{2})<w^{2}>\\
 & = & 2ea^{2}b(b^{2}-a^{2})<w^{2}>\\
 & = & 2ea^{2}b(b^{2}-a^{2})\int\psi^{*}w^{2}\psi d^{3}u &  & (d^{3}u=r^{2}\sin\phi drd\theta d\phi)\\
 & = & 2ea^{2}b(b^{2}-a^{2})\int\psi^{*}\psi r^{2}\cos^{2}\phi r^{2}\sin\phi drd\theta d\phi &  & (w=r\cos\phi,\,\phi\in[0,\pi])\\
 & = & 2ea^{2}b(b^{2}-a^{2})\int_{0}^{2\pi}d\theta\int_{0}^{1}r^{4}dr\int_{0}^{\pi}\cos^{2}\phi\sin\phi d\phi\\
 & = & 2ea^{2}b(b^{2}-a^{2})(2\pi)(\frac{1}{5})(-\frac{1}{3}\cos^{3}\phi\vert_{0}^{\pi})\\
 & = & \frac{4\pi ea^{2}b(b^{2}-a^{2})}{5}(-\frac{(-1)}{3}+\frac{1}{3})\\
 & = & \frac{8\pi ea^{2}b(b^{2}-a^{2})}{15}
\end{aligned}
\]

\end_inset


\end_layout

\begin_layout Standard
This is not the desired answer...!
\end_layout

\begin_layout Problem
For the scattering of 10MeV alpha particles from Au nuclei, calculate the
 total scattering cross section for scattering at angles >1
\begin_inset Formula $^{\circ}$
\end_inset

, >5
\begin_inset Formula $^{\circ}$
\end_inset

, >20
\begin_inset Formula $^{\circ}$
\end_inset

.
 Calculate the differential scattering cross section for scattering at 1
\begin_inset Formula $^{\circ}$
\end_inset

, 5
\begin_inset Formula $^{\circ}$
\end_inset

 and 20
\begin_inset Formula $^{\circ}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma=\pi Z^{2}\Big(\frac{ke^{2}}{KE}\Big)^{2}\Big(\frac{1+\cos\theta}{1-\cos\theta}\Big)
\]

\end_inset


\end_layout

\begin_layout Standard
Au nuclei implies 
\begin_inset Formula $Z=79$
\end_inset

.
 
\begin_inset Formula $k=9*10^{9}\frac{Nm^{2}}{C^{2}}$
\end_inset

.
 
\begin_inset Formula $KE=10MeV=1.6*10^{-12}J$
\end_inset

.
 
\begin_inset Formula $e=1.6*10^{-19}C$
\end_inset

.
\end_layout

\begin_layout Standard
For angles >
\begin_inset Formula $\theta=1^{\circ}$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma=\pi(79)^{2}\Big(\frac{9*10^{9}(1.6*10^{-19})^{2}}{1.6*10^{-12}}\Big)^{2}\Big(\frac{1+cos(1)}{1-cos(1)}\Big)=5.338*10^{-24}m^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
Similarly, for angles > 
\begin_inset Formula $\theta=5^{\circ}$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma=\pi(79)^{2}\Big(\frac{9*10^{9}(1.6*10^{-19})^{2}}{1.6*10^{-12}}\Big)^{2}\Big(\frac{1+cos(5)}{1-cos(5)}\Big)=2.13*10^{-25}m^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
and for angles >
\begin_inset Formula $\theta=20^{\circ}$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\sigma=\pi(79)^{2}\Big(\frac{9*10^{9}(1.6*10^{-19})^{2}}{1.6*10^{-12}}\Big)^{2}\Big(\frac{1+cos(20)}{1-cos(20)}\Big)=1.308*10^{-26}m^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
Now, the differential scattering cross-section is given by
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{d\sigma}{d\Omega}=\Big(kzZe^{2}\Big)^{2}\Big(\frac{1}{4T_{\alpha}}\Big)^{2}\frac{1}{\sin^{4}\big(\frac{\theta}{2}\big)}
\]

\end_inset


\end_layout

\begin_layout Standard
For alpha particles, z=2.
 For 
\begin_inset Formula $\theta=1^{\circ}$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{d\sigma}{d\Omega}=\Big(9*10^{9}(2)(79)(1.6*10^{-19})^{2}\Big)^{2}\Big(\frac{1}{4(1.6*10^{-12})}\Big)^{2}\frac{1}{\sin^{4}\big(\frac{1^{\circ}}{2}\big)}=5.58*10^{-21}m^{2}/sr
\]

\end_inset


\end_layout

\begin_layout Standard
For 
\begin_inset Formula $\theta=5^{\circ}$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{d\sigma}{d\Omega}=\Big(9*10^{9}(2)(79)(1.6*10^{-19})^{2}\Big)^{2}\Big(\frac{1}{4(1.6*10^{-12})}\Big)^{2}\frac{1}{\sin^{4}\big(\frac{5^{\circ}}{2}\big)}=8.94*10^{-24}m^{2}/sr
\]

\end_inset


\end_layout

\begin_layout Standard
For 
\begin_inset Formula $\theta=20^{\circ}$
\end_inset

,
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{d\sigma}{d\Omega}=\Big(9*10^{9}(2)(79)(1.6*10^{-19})^{2}\Big)^{2}\Big(\frac{1}{4(1.6*10^{-12})}\Big)^{2}\frac{1}{\sin^{4}\big(\frac{20^{\circ}}{2}\big)}=3.56*10^{-26}m^{2}/sr
\]

\end_inset


\end_layout

